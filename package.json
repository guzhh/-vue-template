		"@vicons/ionicons5": "^0.12.0",
		"@vicons/tabler": "^0.12.0",
		"@vueuse/core": "^9.12.0",
		"@wangeditor/editor": "^5.1.23",
		"@wangeditor/editor-for-vue": "^5.1.12",
		"ace-builds": "^1.16.0",
		"axios": "^1.3.2",
		"dayjs": "^1.11.7",
		"joi": "^17.7.0",
		"mitt": "^3.0.0",
		"naive-ui": "^2.34.3",
		"nanoid": "^4.0.1",
		"nprogress": "^0.2.0",
		"pinia": "^2.0.30",
		"pinia-plugin-persist": "^1.0.0",
		"qs": "^6.11.0",
		"store": "^2.0.12",
		"vfonts": "^0.0.3",
		"vue": "^3.2.47",
		"vue-qr": "^4.0.9",
		"vue-router": "^4.1.6",
		"vue3-print-nb": "^0.1.4",
		"vuedraggable": "^4.1.0",
		"vxe-table": "^4.3.9",
		"xe-utils": "^3.5.7"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.4",
		"@commitlint/config-conventional": "^17.4.4",
		"@types/node": "^18.11.18",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vitejs/plugin-vue-jsx": "^3.0.0",
		"commitizen": "^4.3.0",
		"cross-env": "^7.0.3",
		"cz-git": "^1.5.1",
		"debug": "^4.3.4",
		"eslint": "8.2.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.25.2",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-vue": "9.3.0",
		"husky": "^8.0.3",
		"koa": "^2.14.1",
		"koa-bodyparser": "^4.3.0",
		"koa-cors": "^0.0.16",
		"koa-logger": "^3.2.1",
		"koa-onerror": "^4.2.0",
		"koa-router": "^12.0.0",
		"koa-static": "^5.0.0",
